{
  "expo": {
    "name": "Toto Vaccine",
    "slug": "toto-vaccine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.verynice.totovaccine"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "android": {
      "package": "com.verynice.totovaccine"
    },
    "description": "An app to schedule and remind mothers of newly born babies of their vaccination schedule.",
    "githubUrl": "https://github.com/Mugambi-Ian/e-toto-vaccine.git"
  }
}
